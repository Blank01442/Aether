say "--- Aether v2.2 Performance Benchmark ---"

set start to time
say "Calculating 10 million operations..."

set count to 0
repeat 10000000 times {
    set count to count + 1
}

set end to time
set duration to end - start

say "Finished 10,000,000 iterations!"
say "Time taken (ms):"
say duration

say "--- Testing Randomness ---"
repeat 5 times {
    set r to random(1, 100)
    say r
}

say "Benchmark Complete."
